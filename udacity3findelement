def find_element(p, t):
    i = 0
    while i<len(p):
        if p[i] == t:
            return i
        i = i + 1
    return -1
    
    
    
def find_element(p, t):
    i = 0
    for e in p:
        if e == t:
            return i
        i = i + 1
    return -1
    
    
    
***from codeacademy***
def count(sequence, item):
	counter = 0
	for i in sequence:
		if i == item:
			counter += 1
	return counter
            
